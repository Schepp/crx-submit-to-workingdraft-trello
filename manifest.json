{
	"name": "Submit as Card to Workingdraft Trello Board",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Submit current URL to Trello, creating a new card in the newest Workingdraft board's Hype-list.",
	"icons": { 
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com https://api.trello.com; object-src 'self'",
	"permissions": [
		"tabs",
		"https://code.jquery.com/",
		"https://trello.com/",
		"https://api.trello.com/"
	]
}